<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
		  http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
           version="3.0">

    <servlet>
        <servlet-name>PaquServlet</servlet-name>
        <servlet-class>Servlet.PaquServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LiangHuaServlet</servlet-name>
        <servlet-class>Servlet.LiangHuaServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ResultServlet</servlet-name>
        <servlet-class>Servlet.ResultServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TrainServlet</servlet-name>
        <servlet-class>Servlet.TrainServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>PaquServlet</servlet-name>
        <url-pattern>/PaquServlet.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LiangHuaServlet</servlet-name>
        <url-pattern>/LiangHuaServlet.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ResultServlet</servlet-name>
        <url-pattern>/ResultServlet.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TrainServlet</servlet-name>
        <url-pattern>/TrainServlet.do</url-pattern>
    </servlet-mapping>
</web-app>
